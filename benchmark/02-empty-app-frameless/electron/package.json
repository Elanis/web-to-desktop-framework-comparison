{
  "name": "02-empty-app-frameless",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron --enable-logging .",
    "build-windows": "electron-packager . 02-empty-app-frameless --ignore=node_module --overwrite --platform=win32 --arch=ia32,x64,arm64 --out=build/",
    "build-linux": "electron-packager . 02-empty-app-frameless --ignore=node_module --overwrite --platform=linux --arch=x64,armv7l,arm64 --out=build/",
    "build-mac": "electron-packager . 02-empty-app-frameless --ignore=node_module --overwrite --platform=darwin,mas --arch=x64,arm64 --out=build/",
    "build": "npm run build-windows && npm run build-linux && npm run build-mac",
    "build-ci": "sh -c 'if [ \"$RUNNER_OS\" = \"Linux\" ] ; then npm run build-linux ; elif [ \"$RUNNER_OS\" = \"Windows\" ] ; then npm run build-windows ; elif [ \"$RUNNER_OS\" = \"macOS\" ] ; then npm run build-mac ; fi'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^19.0.4",
    "electron-packager": "^15.5.1"
  }
}
