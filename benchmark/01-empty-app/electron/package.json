{
  "name": "01-empty-app",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-windows": "electron-packager . 01-empty-app --ignore=node_module --overwrite --platform=win32 --arch=ia32,x64,armv7l,arm64,mips64el --out=build/",
    "build-linux": "electron-packager . 01-empty-app --ignore=node_module --overwrite --platform=linux --arch=ia32,x64,armv7l,arm64,mips64el --out=build/",
    "build-darwin": "electron-packager . 01-empty-app --ignore=node_module --overwrite --platform=darwin --arch=ia32,x64,armv7l,arm64,mips64el --out=build/",
    "build-mas": "electron-packager . 01-empty-app --ignore=node_module --overwrite --platform=mas --arch=ia32,x64,armv7l,arm64,mips64el --out=build/",
    "build": "npm run build-windows && npm run build-linux && npm run build-darwin && npm run build-mas"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-packager": "^15.3.0"
  }
}
